import java.util.Scanner;

public class SwiftInterpreter {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Enter a simple Swift-like operation (e.g., print(2 + 3)): ");
        String input = scanner.nextLine();

        if (input.startsWith("print(") && input.endsWith(")")) {
            String expression = input.substring(6, input.length() - 1).trim();
            try {
                if (expression.contains("+")) {
                    String[] parts = expression.split("\\+");
                    int result = Integer.parseInt(parts[0].trim()) + Integer.parseInt(parts[1].trim());
                    System.out.println(result);
                } else {
                    System.out.println("Unsupported operation.");
                }
            } catch (Exception e) {
                System.out.println("Error interpreting the input.");
            }
        } else {
            System.out.println("Invalid Swift-like input.");
        }

        scanner.close();
    }
}
